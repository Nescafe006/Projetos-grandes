{
  "expo": {
    "name": "supabase",
    "slug": "supabase",
    "version": "1.0.0",
    "orientation": "portrait",
  
    "scheme": "supabase",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
    
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
  
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}